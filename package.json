{
  "name": "rule-validator",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.config.json",
    "start-server": "node server/index.js --watch",
    "build": "ng build",
    "build:prod": "ng lint & ng build --base-href=/ndw/ --configuration=production",
    "build:dev": "ng lint & ng build --base-href=/ndw/ --configuration=development",
    "build:uat": "ng lint & ng build --base-href=/ndw/ --configuration=deploy",
    "watch": "ng build --watch --configuration development",
    "seed": "node seed.js",
    "test": "ng test",
    "test:coverage": "ng test --no-watch --code-coverage",
    "lint": "ng lint"
  },
  "private": true,
